nslookup 
ipconfig /flushdns

cd ../..

# Required to install modules from the PowerShell Gallery
Install-Module -Name PowerShellGet -Repository PSGallery -Force

# Install the AzFilesHybrid module
Install-Module -Name AzFilesHybrid -Repository PSGallery -AllowClobber -Force

cd .\AzFilesHybrid

Import-Module -Name AzFilesHybrid

Connect-AzAccount -UseDeviceAuthentication

Join-AzStorageAccountForAuth `
    -ResourceGroupName "AVDRG" `
    -Name "avdstorage303" `
    -DomainAccountType "ComputerAccount" `
    -OrganizationalUnitDistinguishedName "CN=Computers,DC=vpoint,DC=info"

Debug-AzStorageAccountAuth -StorageAccountName iplplayers -ResourceGroupName AVD-RG -Verbose

